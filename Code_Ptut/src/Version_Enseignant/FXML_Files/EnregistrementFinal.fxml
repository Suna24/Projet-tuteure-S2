<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.effect.*?>
<?import javafx.scene.canvas.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.media.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<BorderPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Version_Enseignant.All_Controllers.Controller_Enregistrement_Final">
   <top>
      <VBox prefHeight="32.0" prefWidth="1080.0" BorderPane.alignment="TOP_LEFT">
			<children>
				<MenuBar snapToPixel="false">
					<menus>
						<Menu mnemonicParsing="false" text="Exercice">
							<items>
								<MenuItem mnemonicParsing="false" text="Nouveau" />
								<MenuItem mnemonicParsing="false" onAction="#ouvrir" text="Ouvrir" />
								<SeparatorMenuItem mnemonicParsing="false" />
								<MenuItem mnemonicParsing="false" text="Enregistrer" />
								<MenuItem mnemonicParsing="false" text="Enregistrer sous" />
								<SeparatorMenuItem mnemonicParsing="false" />
								<MenuItem mnemonicParsing="false" onAction="#quitter" text="Quitter" />
							</items>
						</Menu>
						<Menu mnemonicParsing="false" text="Aide">
							<items>
								<MenuItem mnemonicParsing="false" text="Tutoriel" />
								<MenuItem mnemonicParsing="false" text="A propos" />
							</items>
						</Menu>
						<Menu mnemonicParsing="false" text="Paramètres">
                     <items>
                        <MenuItem mnemonicParsing="false" onAction="#pageParametres" text="Préférences" />
                        <CheckMenuItem mnemonicParsing="false" text="DarkMode" />
                     </items></Menu>
					</menus>
					<VBox.margin>
						<Insets />
					</VBox.margin>
				</MenuBar>
			</children>
		</VBox>
   </top>
   <center>
      <VBox prefHeight="404.0" prefWidth="1080.0" BorderPane.alignment="CENTER">
         <children>
            <HBox alignment="CENTER" prefHeight="68.0" prefWidth="1080.0">
               <children>
                  <Text fx:id="recupScene" strokeType="OUTSIDE" strokeWidth="0.0" text="Votre exercice a bien été enregistré !!">
                     <font>
                        <Font size="25.0" />
                     </font>
                  </Text>
               </children>
               <VBox.margin>
                  <Insets bottom="50.0" top="100.0" />
               </VBox.margin>
            </HBox>
            <HBox alignment="BOTTOM_RIGHT" prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#annuler" text="Ok">
                     <HBox.margin>
                        <Insets bottom="25.0" right="50.0" />
                     </HBox.margin>
                  </Button>
               </children>
               <VBox.margin>
                  <Insets left="50.0" right="50.0" />
               </VBox.margin>
            </HBox>
         </children>
      </VBox>
   </center>
</BorderPane>
